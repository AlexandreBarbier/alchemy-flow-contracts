{
  "name": "flow-alchemy-contracts",
  "version": "1.0.0",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "ts-node-dev --transpileOnly --inspect -- src/index.ts",
    "format-file": "prettier --write",
    "format": "prettier --write '**/*.{html,js,json,md,ts}'",
    "jest-watch": "jest --watch --notify",
    "jest": "jest",
    "lint-file": "eslint --fix",
    "lint": "eslint --fix '**/*.ts'",
    "shell": "ts-node --transpile-only",
    "start": "pm2-runtime start ecosystem.config.js",
    "test": "npm-run-all typecheck lint jest",
    "typecheck": "tsc --noEmit",
    "typecheck-watch": "tsc --noEmit --watch",
    "nft-metadata": "ts-node-dev --transpileOnly --inspect -- src/nftMetadata.ts",
    "nft-id": "ts-node-dev --transpileOnly --inspect -- src/nftIdsByAccount.ts"
  },
  "dependencies": {
    "@onflow/fcl": "0.0.77",
    "@onflow/types": "0.0.5",
    "@types/express": "^4.17.13",
    "@types/node": "^13.1.4",
    "express": "^4.17.1",
    "pm2": "^4.2.3",
    "transducist": "^2.2.0",
    "tslib": "^1.10.0",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@types/jest": "^25.1.2",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.19.1",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.0",
    "ts-node": "^8.5.4",
    "ts-node-dev": "^1.0.0-pre.44"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,json,md}": [
      "npm run format-file"
    ],
    "**/*.ts": [
      "npm run lint-file --fix",
      "npm run format-file"
    ]
  }
}
